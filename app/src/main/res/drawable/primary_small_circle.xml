<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:top="5dp" >
    <shape android:shape="oval">
      <gradient
        android:type="radial"
        android:gradientRadius="45%"
        android:centerY="0.49"
        android:endColor="@color/transparentBlack"
        android:centerColor="#000"
        android:startColor="#000" />
    </shape>
  </item>
  <item android:top="5dp" android:right="5dp" android:left="5dp" android:bottom="5dp">
    <shape android:shape="oval">
      <solid android:color="@color/colorPrimary" />
    </shape>
  </item>
  <item android:top="5dp" android:right="5dp" android:left="5dp" android:bottom="5dp">
    <shape android:shape="oval">
      <gradient
        android:type="radial"
        android:gradientRadius="150%"
        android:endColor="@color/semiTransparentBlack"
        android:startColor="@color/transparentBlack" />
    </shape>
  </item>
</layer-list>
